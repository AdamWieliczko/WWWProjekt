@model MVCInterProject.Models.Package

@if(Model != null && Model.Shippings != null)
{
    @for(int i = 0; i < @Model.Shippings.Count(); i++) {
        @Html.EditorFor(@Model => @Model.Shippings[i])
        <input type="button" class="btn button-primary" onclick="refresh(this)" data-shipping-index=@i value="Delete shipping above"/>
    }
}